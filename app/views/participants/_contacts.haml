%table.table.table-striped.table-hover
  %thead
    %tr
      %th
        Name
      %th
        Times contacted
      %th
        Last contacted

      

      
  %tbody
    -@participant.contact_records.order(timesContacted: :desc).each do |g|
      %tr
        %td
          =g.displayName
        %td
          =g.timesContacted
        %td
          =g.timesContacted > 0 ? DateTime.strptime((g.lastTimeContacted / 1000).to_s,'%s').in_time_zone("Auckland").strftime('%c') : "" 

        
  